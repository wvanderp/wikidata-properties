{
  "name": "wikidata-properties",
  "version": "1.0.0",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "dependencies": {},
  "devDependencies": {
    "wtf_wikipedia": "^8.5.1",
    "@types/node": "^14.6.4",
    "copyfiles": "^2.3.0",
    "parcel": "^1.12.4",
    "rimraf": "^3.0.2",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.2"
  },
  "scripts": {
    "scrape": "ts-node src/scrape.ts",
    "build:parcel": "parcel build src/index.ts --no-source-maps",
    "build:copy": "copyfiles -f dist/src/* dist/",
    "build:tsc": "tsc -d --outDir dist",
    "build:clean": "rimraf dist/data && rimraf dist/src",
    "build": "rimraf dist && npm run scrape && npm run build:tsc && npm run build:copy && npm run build:parcel && npm run build:clean"
  }
}
